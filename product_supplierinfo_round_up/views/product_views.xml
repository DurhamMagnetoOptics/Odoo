<odoo>
    <record id="product_supplierinfo_search_view_inherit_stock_move_push_leftover" model="ir.ui.view">
        <field name="name">Round Up</field>
        <field name="model">product.supplierinfo</field>
        <field name="inherit_id" ref="product.product_supplierinfo_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//label[@for='min_qty']" position="attributes">
                <attribute name="string">Minimum Order Quantity</attribute>
            </xpath>             
            <xpath expr="//label[@for='price']" position="before">
                <label for="increment_qty"/>
                <div class="o_row">
                    <field name="increment_qty"/>
                    <field name="product_uom" groups="uom.group_uom"/>
                </div>
                <label for="round_up"/>
                <div class="o_row">
                    <field name="round_up"/>
                </div>
            </xpath>
        </field>
    </record>
</odoo>



